package Exercicios;

import java.util.Scanner;

public class teste {

	public static void main(String[] args) {
		// Escreva	um programa	para ler o ano de nascimento de uma pessoa e escrever uma mensagem que	diga se	ela	poderá	
		//ou não votar este ano pra voto 
		
		Scanner sc = new Scanner(System.in);
		
		int x;
		int y;
		
		System.out.println("Digite o valor 1:");
		x = sc.nextInt();
		System.out.println("Digite o valor 2:");
		y = sc.nextInt();
		
		if( x > y ) {
			System.out.println("O maior valor digitado é o valor 1: " + x);
		}
		else {
			System.out.println("O maior valor digitado é o valor 2: " + y);
		}
		
		sc.close();

	}

}
