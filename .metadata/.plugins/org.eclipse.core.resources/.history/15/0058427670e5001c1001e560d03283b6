package Estruturas_condicionais;

public class _02_Expressoes_Logicas {

	public static void main(String[] args) {
		// Expressões Lógicas
		
		                                       // Tabela operador E   |
		
		// OPERADOR        SIGNIFICADO         |__    A   B   A&&B  __|
		//                                      _____             _____
		//    &&               E               |      F   F     F     |
		//	  ||               OU              |      F   V     F     |
		//	  !                NÃO             |      V   F     F     |
        //                                     |      V   V     V     |

//*****************************************************************************
		
		//EXEMPLO [ x_el = variavel x + expressão lógica ]
		
// Supondo que x_el = 5 Então responda: x_el <= 20 && x_el == 10 | x_el > 0 && x_el != 3 | x_el <=20 && x_el == 10 && x_el != 3
		
		int x_el = 5;
		
		System.out.println(x_el <= 20 && x_el == 10);
		System.out.println(x_el > 0 && x_el != 3);
		System.out.println(x_el <=20 && x_el == 10 && x_el != 3);
	}

}
